        "react",
        "node",
        "express",
        "mongodb"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^7.6.3"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}