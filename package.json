{
    "name": "egyptian-ecommerce",
    "version": "1.0.0",
    "description": "Egyptian E-Commerce Store",
    "main": "index.js",
    "scripts": {
        "start": "node backend/server.js",
        "server": "nodemon backend/server.js",
        "client": "npm start --prefix frontend",
